    "outcomesModule": {
      "primaryOutcomes": [
        {
          "measure": "Oral food challenge to fenugreek",
          "description": "Astier's score 0 to 4 (0=negative, 1 and 2=mild to moderate reaction, 3 and 4 = severe reaction)",
          "timeFrame": "2017-2022"
        },
        {
          "measure": "skin prick-test to fenugreek",
          "description": "skin prick test positive if wheal size superior to 3mm",
          "timeFrame": "2017-2022"
        },
        {
          "measure": "specific IgE to fenugreek",
          "description": "specific IgE positive if value superior to 0.35kU/L",
          "timeFrame": "2017-2022"
        }
      ],
      "secondaryOutcomes": [
        {
          "measure": "severity of allergy",
          "description": "reactogenic dose (from 1 to 1000mg of fenugreek)",
          "timeFrame": "2017-2022"
        },
        {
          "measure": "molecular profile for peanut allergy",
          "description": "IgE values for rAra h1, h2, h3, h6, h8, h9",
          "timeFrame": "2017-2022"
        }
      ]
    },
