    "outcomesModule": {
      "primaryOutcomes": [
        {
          "measure": "Exposure to pholcodine",
          "description": "Exposure is measured by autoquestionnaire, patient's pharmaceutical file and drug history by the pharmacist's",
          "timeFrame": "within the 12 months before the anesthetic procedure"
        }
      ],
      "secondaryOutcomes": [
        {
          "measure": "Anti-pholcodine IgE, anti-ammonium IV IgE and total IgE levels between the case and control groups.",
          "description": "Cases: 6 to 12 weeks after the general anesthetic procedure (corresponding to the day of inclusion) Controls: during their hospitalisation after the general anesthetic procedure (Maximum 90 days after the general anesthetic procedure, corresponding to the day of inclusion)",
          "timeFrame": "Between 1 day to 12 weeks after the general anesthetic procedure"
        },
        {
          "measure": "Concordance between exposure to pholcodine in cases and controls, by means of a patient self-questionnaire on the one hand and, on the other hand, by a computerized drug history, supplemented where relevant by the drug master file.",
          "timeFrame": "within the 12 months preceeding the general anesthesia"
        },
        {
          "measure": "Impact of non subjective sources in pholcodine exposure assessment",
          "description": "We will study if pholcodine exposure criteria measured by autoquestionnaire is modified by taking into account non subjectives sources which are the patient's pharmaceutical file and his drug history by his pharmacist's",
          "timeFrame": "within the 12 months preceeding general anesthetic procedure"
        },
        {
          "measure": "Association between exposure to pholcodine and the presence/levels of pholcodine-specific IgE, reflecting sensitisation to pholcodine.",
          "description": "IgE measurements: total IgE, IgE specific for pholcodine, quaternary ammoniums (KU/L)",
          "timeFrame": "within the 12 months preceding the general anesthetic procedure"
        },
        {
          "measure": "NMBA and pholcodine cross-sensitisation by testing skin reactions to pholcodine in case patients allergic to one NMBA.",
          "description": "Only for Cases :\n\nIntradermal tests with diluted pholcodine. Intradermal tests with diluted NMBAs (as usual)",
          "timeFrame": "6 to 12 weeks after the general anesthetic procedure"
        }
      ]
    },
    "eligibilityModule": {
      "eligibilityCriteria": "Inclusion Criteria:\n\n* Male or female, age ≥ 2 years old.\n* Referred to anaesthesia-allergy consultation within (approximately) 6 to 12 weeks of occurrence of peranaesthetic anaphylactic reaction during anaesthesia with administration of NMBAs (only for case patient).\n* Having given his/her consent (or the 2 parents consent for minors).\n* Affiliated with a social security scheme or dependent.\n* Able to answer a medicinal product intake questionnaire\n* In a clinical condition compatible with skin tests (no skin disease or psychiatric illness, etc.) (only for case patient)\n* Having stopped any anti-histamine treatment at least 8 days previously (only for case patient).\n* With positive skin test for the suspected NMBA (ony for case patient).\n* Patient anaesthetised in a control recruitment centre (only for control patients)\n* Having undergone anaesthesia with NMBA injection, without onset of peranaesthetic anaphylactic reaction whatever his medical history (only for control patients)\n\nExclusion Criteria:\n\n* Patients who have refused, or are unable to give their consent\n* Patients who have had negative control skin tests\n* Anaphylaxis suspected to be related to other drugs given on induction of anaesthesia, eg. antibiotics\n* Having presented with anaphylactic reaction during a previous anaesthesia without NMBA injection\n* Pregnant females at inclusion or during 12 months before anaesthesia",
      "healthyVolunteers": false,
      "sex": "ALL",
      "minimumAge": "2 Years",
      "stdAges": [
        "CHILD",
        "ADULT",
        "OLDER_ADULT"
      ]
