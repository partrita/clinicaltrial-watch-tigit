    "outcomesModule": {
      "primaryOutcomes": [
        {
          "measure": "Specific Immunoglobulin E (IgE)",
          "description": "Blood samples were taken from study and control group for the determination of Specific IgE panel against six different allergens: (1) fungi, (2) grass pollens, (3) tree pollens, (4) wild herbs, (5) mite 1, (6) mite 2. For all of specific IgE panels, \\<0,7 RU/ml was negative; and \\>0,7 RU/ml was Positive.",
          "timeFrame": "1 day"
        },
        {
          "measure": "Nasal Symptom Scores",
          "description": "Symptoms were assessed with a questionnaire for the presence and severity of allergic rhinitis (AR) symptoms on a scale of 0-3 (0 = none, 1 = mild, 2 = moderate, 3 = severe). Symptoms of AR (Nasal discharge, nasal congestion, itching and sneezing) were graded between 0-3.",
          "timeFrame": "1 day"
        }
      ],
      "otherOutcomes": [
        {
          "measure": "İnferior turbinate color and edema",
          "description": "inferior concha colour (0=natural, 1=pale, 2=bluish and 3=severely pale or bluish) and turbinate edema were assessed during ENT examination on 0-3 scale (0 = no, 1 = mild, 2 = moderate, 3 = severe).",
          "timeFrame": "1 day"
        }
      ]
    },
    "eligibilityModule": {
      "eligibilityCriteria": "Inclusion Criteria:\n\n* patients with migraine\n\nExclusion Criteria:\n\n* other neurologic disorders that may contribute to headaches, such as stroke, cerebral palsy, trigeminal neuralgia, and seizure disorder\n* also acute and chronic sinusitis.",
